{
  "name": "evolut",
  "version": "0.1.2",
  "description": "Design of artificial animals with evolutionary algorithms",
  "private": true,
  "main": "main.js",
  "scripts": {
    "assert": "mocha test/**/*.js",
    "build": "electron-packager ./ --platform=all --arch=x64 --asar=true --ignore='node_modules/*' --ignore=doc --ignore=test --out=./dist",
    "doc": "jsdoc -d doc -r src",
    "lint": "eslint src/**/*.js test/**/*.js",
    "esdoc": "esdoc -c .esdocrc",
    "start": "electron .",
    "test": "npm run lint && npm run assert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedigerf/evolut.git"
  },
  "keywords": [
    "algorithms",
    "evolution",
    "evolutionary algorithms",
    "phyiscs",
    "simulation"
  ],
  "author": "evolut team",
  "contributors": [
    "Fabian Hediger <hedigerf@gmail.com>",
    "Florian Tanner <florian.tanner@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hedigerf/evolut/issues"
  },
  "homepage": "https://github.com/hedigerf/evolut/#readme",
  "dependencies": {
    "app-root-path": "latest",
    "bootstrap": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-register": "latest",
    "config": "latest",
    "electron-canvas-to-buffer": "latest",
    "immutable" : "latest",
    "jquery": "latest",
    "log4js": "latest",
    "nconf": "latest",
    "p2": "latest",
    "p2Pixi": "git+https://github.com/TomWHall/p2Pixi.git#master",
    "partial.lenses": "2.0.x",
    "pixi.js": "latest",
    "point-in-polygon": "latest",
    "ramda": "0.20.x",
    "random-js": "latest",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "chai": "latest",
    "chai-fuzzy": "latest",
    "chai-immutable": "latest",
    "electron-packager": "latest",
    "electron-prebuilt": "0.37.3",
    "esdoc": "latest",
    "eslint": "latest",
    "jsdoc": "latest",
    "mocha": "latest"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
